import { Directive } from '@angular/core';
/**
 * Adds properties and events to drag handle elements
 *
 * @export
 */
export class DragHandleDirective {
}
DragHandleDirective.decorators = [
    { type: Directive, args: [{ selector: '[ngxDragHandle]' },] }
];
